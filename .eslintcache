"react\";\nimport { Route, Switch } from \"react-router-dom\";\n\nimport PrivateRoute from \"./components/HOC/PrivateRoute\";\nimport Home from \"./containers/Home\";\nimport Signin from \"./containers/Signin\";\nimport Signup from \"./containers/Signup\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { getInitialData, isUserLoggedIn } from \"./redux/actions\";\nimport Product from \"./containers/Products\";\nimport Orders from \"./containers/Orders\";\nimport Category from \"./containers/Category\";\nimport NewPage from \"./containers/NewPage\";\n\nfunction App() {\n  const auth = useSelector((state) => state.auth);\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    //Read Comment Below\n    if (!auth.authenticated) {\n      dispatch(isUserLoggedIn());\n    }\n\n    if (auth.authenticated) {\n      dispatch(getInitialData());\n    }\n  }, [auth.authenticated]);\n\n  return (\n    <div className=\"App\">\n      <Switch>\n        <PrivateRoute path=\"/\" exact component={Home} />\n        <PrivateRoute path=\"/page\" component={NewPage} />\n\n        <PrivateRoute path=\"/category\" exact component={() => <Category />} />\n\n        <PrivateRoute path=\"/products\" component={() => <Product />} />\n        <PrivateRoute path=\"/orders\" component={() => <Orders />} />\n\n        <Route path=\"/signin\" exact component={Signin} />\n        <Route path=\"/signup\" exact component={Signup} />\n      </Switch>\n    </div>\n  );\n}\n\nexport default App;\n\n/*\n\n(1)\n - when admin time /signin in URL page reload and Store became null\n - so that's why authenticated is false\n - in action we check that\n - OK, admin is logged in which means token is stord in localstorage\n - so we check that is token is there than we update the authenticated to TRUE\n - so we can achive REDIRECT functionality\n\n\n(2)\n  - when we are on Dashboard and click on singin we see signin page for 1/5 milisecond\n  - WHY..? because we are setting on Dashboard and out checking happen on sing in page \n  - Solution..?\n  - checking happen on Dashboard level so copy useEffect from here to Dashboard\n\n(3)\n  - we need to perform checking on every page insted of that \n  - we remove <Route> from App.js to index.js\n  - and write checking useEffect on TOP Level \n\n*/\n","D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\redux\\store\\index.js",[],"D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\components\\HOC\\PrivateRoute.js",[],"D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\redux\\actions\\index.js",[],"D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\containers\\Home\\index.js",[],"D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\containers\\Signup\\index.js",["164","165","166"],"import React, { useState } from \"react\";\nimport { Button, Col, Container, Form, Row } from \"react-bootstrap\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Redirect } from \"react-router-dom\";\n\nimport Layout from \"../../components/Layout\";\nimport Input from \"../../components/UI_Common/Input\";\nimport { registration } from \"../../redux/actions\";\n\nconst Signup = (props) => {\n  const [firstName, setFirstName] = useState(\"\");\n  const [lastName, setLastName] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n  const [error, setError] = useState(\"\");\n\n  const auth = useSelector((state) => state.auth);\n  const user = useSelector((state) => state.user);\n\n  const state = useSelector((state) => state.auth);\n\n  const dispatch = useDispatch();\n\n  const userSignup = (e) => {\n    e.preventDefault();\n\n    const user = {\n      firstName,\n      lastName,\n      email,\n      password,\n    };\n\n    dispatch(registration(user));\n  };\n\n  if (auth.authenticated) {\n    return <Redirect to=\"/\" />;\n  }\n\n  if (user.loading) {\n    return <p>Loading...</p>;\n  }\n\n  return (\n    <Layout>\n      <Container>\n        {user.message}\n        <Row style={{ marginTop: \"50px\" }}>\n          <Col md={{ span: 6, offset: 3 }}>\n            <Form onSubmit={userSignup}>\n              <Row>\n                <Col md={6}>\n                  <Input\n                    label=\"First Name\"\n                    placeholder=\"First Name\"\n                    value={firstName}\n                    type=\"text\"\n                    onChange={(e) => setFirstName(e.target.value)}\n                  />\n                </Col>\n\n                <Col md={6}>\n                  <Input\n                    label=\"Last Name\"\n                    placeholder=\"Last Name\"\n                    value={lastName}\n                    type=\"text\"\n                    onChange={(e) => setLastName(e.target.value)}\n                  />\n                </Col>\n              </Row>\n\n              <Input\n                label=\"Email\"\n                placeholder=\"Email\"\n                value={email}\n                type=\"email\"\n                onChange={(e) => setEmail(e.target.value)}\n              />\n\n              <Input\n                label=\"Password\"\n                placeholder=\"Password\"\n                value={password}\n                type=\"password\"\n                onChange={(e) => setPassword(e.target.value)}\n              />\n\n              <Button variant=\"primary\" type=\"submit\">\n                Submit\n              </Button>\n            </Form>\n          </Col>\n        </Row>\n      </Container>\n    </Layout>\n  );\n};\nexport default Signup;\n","D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\containers\\Signin\\index.js",["167"],"import React, { useEffect, useState } from \"react\";\nimport { Button, Col, Container, Form, Row } from \"react-bootstrap\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Redirect } from \"react-router-dom\";\n\nimport Layout from \"../../components/Layout\";\nimport Input from \"../../components/UI_Common/Input\";\nimport { login } from \"../../redux/actions\";\n\nconst Signin = (props) => {\n  const [email, setEmail] = useState(\"\");\n  const [password, setPassword] = useState(\"\");\n\n  const dispatch = useDispatch();\n  const auth = useSelector((state) => state.auth);\n  if (auth.authenticated) {\n    return <Redirect to=\"/\" />;\n  }\n\n  const userLogin = (e) => {\n    e.preventDefault();\n    const user = {\n      email,\n      password,\n    };\n    dispatch(login(user));\n  };\n\n  return (\n    <Layout>\n      <Container>\n        <Row style={{ marginTop: \"50px\" }}>\n          <Col md={{ span: 6, offset: 3 }}>\n            <Form onSubmit={userLogin}>\n              <Input\n                label=\"Email\"\n                placeholder=\"Email\"\n                value={email}\n                type=\"email\"\n                onChange={(e) => setEmail(e.target.value)}\n              />\n\n              <Input\n                label=\"Password\"\n                placeholder=\"Password\"\n                value={password}\n                type=\"password\"\n                onChange={(e) => setPassword(e.target.value)}\n              />\n\n              <Button variant=\"primary\" type=\"submit\">\n                Submit\n              </Button>\n            </Form>\n          </Col>\n        </Row>\n      </Container>\n    </Layout>\n  );\n};\nexport default Signin;\n","D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\containers\\Category\\index.js",[],"D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\containers\\Orders\\index.js",[],"D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\containers\\Products\\index.js",[],"D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\redux\\reducers\\index.js",[],"D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\redux\\actions\\product.action.js",[],"D:\\Mohammadtalha.Patel\\Homework\\MERN_HOME\\react\\src\\redux\\actions\\auth.actions.js",[],"D:\